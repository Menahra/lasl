{
  "name": "lasl-frontend",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "biome lint src tests",
    "lint:fix": "biome lint --write src tests",
    "format": "biome lint src tests",
    "format:fix": "biome format --write src tests",
    "check:ci": "biome check src tests",
    "check:fix": "biome check --write src tests",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "test:e2e": "playwright test",
    "check:types": "tsc --noEmit",
    "compile:messages": "lingui compile --typescript",
    "extract:messages": "lingui extract"
  },
  "dependencies": {
    "@lingui/core": "^5.6.0",
    "@lingui/react": "^5.6.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-router": "^1.141.2",
    "axios": "^1.13.2",
    "clsx": "^2.1.1",
    "http-status-codes": "^2.3.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@lasl/authentication-service": "workspace:*",
    "@lingui/babel-plugin-lingui-macro": "^5.6.0",
    "@lingui/cli": "^5.6.0",
    "@lingui/vite-plugin": "^5.6.0",
    "@playwright/test": "^1.57.0",
    "@tanstack/router-plugin": "^1.136.8",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "happy-dom": "^20.0.11",
    "msw": "^2.12.4",
    "playwright": "^1.57.0",
    "vite": "7.2.7",
    "vite-plugin-svgr": "^4.5.0",
    "vite-tsconfig-paths": "^6.0.1",
    "vitest": "^4.0.15"
  }
}
