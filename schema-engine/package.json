{
  "name": "schema-engine",
  "version": "0.0.1",
  "scripts": {
    "build": "tsup",
    "lint": "biome lint ./src ./tests",
    "lint:fix": "biome lint --write ./src ./tests",
    "format": "biome lint ./src ./tests",
    "format:fix": "biome format --write ./src ./tests",
    "check:ci": "biome check ./src ./tests",
    "test:watch": "vitest",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "types:check": "tsc --noEmit"
  },
  "dependencies": {
    "ajv": "^8.17.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@vitest/coverage-v8": "^3.0.5",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
}
